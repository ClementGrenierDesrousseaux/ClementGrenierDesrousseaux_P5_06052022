{% extends "baseAdmin.html.twig" %}

{% block title %} Page d'accueil {% endblock %}

{% block content %}
    <div class="admin-display-list-articles">
        <table class="admin-display-article">
            <tr>
                <th>Email</th>
                <th>Nom/Pr√©nom</th>
                <th class="display-desktop">>Contenu</th>
                <th>Date</th>
            </tr>
            {% for message in contactMessage %}
                <tr class="admin-display-article-content">
                    <td>{{ message.emailUserContactForm | raw }}</td>
                    <td>{{ message.nameUserContactForm | raw }}</td>
                    <td class="display-desktop">>{{ message.contentUserContactForm | raw }}</td>
                    <td>{{ message.dateUserContactForm | date("d/m/Y")}}</td>
                </tr>
            {% endfor %}
        </table>
    </div>
{% endblock %}
